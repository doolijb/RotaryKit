{
  "name": "app",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "dev": "vite dev",
    "build": "vite build",
    "preview": "vite preview",
    "test": "concurrently \"npm run test:start-server\" \"npm run test:start-vitest\"",
    "test:start-vitest": "wait-on http://localhost:9998 && vitest --watch --threads false",
    "test:start-server": "NODE_ENV=test vite --port 9998",
    "test:ui": "vitest watch --ui --threads false",
    "check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
    "check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
    "lint": "prettier --plugin-search-dir . --check . && eslint .",
    "format": "prettier --plugin-search-dir . --write .",
    "test:integration": "playwright test",
    "test:unit": "vitest",
    "storybook": "storybook dev -p 6006",
    "sb": "storybook dev -p 6006",
    "story": "histoire dev",
    "build-storybook": "storybook build",
    "db:generate": "drizzle-kit generate:pg --config /srv/www/app/src/lib/server/database/config.ts",
    "db:push": "drizzle-kit push:pg --config /srv/www/app/src/lib/server/database/config.ts",
    "db:studio": "drizzle-kit studio --config /srv/www/app/src/lib/server/database/config.ts",
    "db:drop": "drizzle-kit drop --config /srv/www/app/src/lib/server/database/config.ts",
    "aliases": "node ./.scripts/updateAliases.js"
  },
  "devDependencies": {
    "@babel/core": "^7.22.5",
