{
  "extends": "./.svelte-kit/tsconfig.json",
  "compilerOptions": {
    "baseUrl": "./",
    "allowJs": true,
    "checkJs": true,
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "resolveJsonModule": true,
    "skipLibCheck": true,
    "sourceMap": true,
    "strict": false,
    "paths": {
      "@components": [
        "./src/lib/client/components"
      ],
      "@stores": [
        "./src/lib/shared/client/stores"
      ],
      "@themes": [
        "./src/lib/client/themes"
      ],
      "@utils": [
        "./src/lib/client/utils"
      ],
      "@auth": [
        "./src/lib/server/auth"
      ],
      "@controllers": [
        "./src/lib/server/controllers"
      ],
      "@database": [
        "./src/lib/server/database"
      ],
      "@providers": [
        "./src/lib/server/providers"
      ],
      "@requests": [
        "./src/lib/server/requests"
      ],
      "@constants": [
        "src/lib/shared/constants"
      ],
      "@data": [
        "src/lib/shared/shared/data"
      ],
      "@types": [
        "src/lib/shared/interfaces"
      ],
      "@testing": [
        "./src/lib/shared/testing"
      ],
      "@validation": [
        "src/lib/shared/validation"
      ],
      "$lib": [
        "./src/lib"
      ],
      "$lib/*": [
        "./src/lib/*"
      ]
    }
  }
}